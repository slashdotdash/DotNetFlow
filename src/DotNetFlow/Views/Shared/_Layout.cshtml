<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/style.css")" rel="stylesheet" type="text/css" />
    
    <!--[if lt IE 7]>
    <script src="@Url.Content("~/Scripts/pngfix.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.helper.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Content/basic_ie.css")" rel="stylesheet" type="text/css" />
    <![endif]-->

    <link href="@Url.Content("~/Content/superfish.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/dropdownmenu.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
</head>
<body>
    @Html.Partial("Header")

    <div id="wrapper">
        <div id="content">
            <div class="content_top">
             	<div class="content_bottom">
                    @RenderBody()
                </div>
          	</div>
        </div>

        @Html.Partial("SideBar")
    </div>

    @Html.Partial("Footer")
</body>
</html>